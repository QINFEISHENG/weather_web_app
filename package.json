{
  "name": "weather_web_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "db:local:reset": "rm -f dev.db prisma/dev.db && rm -rf prisma/migrations",
    "prisma:local:generate": "dotenv -e .env.local -- prisma generate --schema prisma/schema.sqlite.prisma",
    "prisma:local:migrate": "dotenv -e .env.local -- prisma migrate dev --name init --schema prisma/schema.sqlite.prisma",
    "prisma:prod:generate": "prisma generate --schema prisma/schema.postgres.prisma",
    "prisma:prod:deploy": "prisma migrate deploy --schema prisma/schema.postgres.prisma",
    "postinstall": "prisma generate --schema prisma/schema.postgres.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "@sentry/nextjs": "^10.38.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jest": "^29.7.0",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
}
